    "# leetcode - 151. Reverse Words in a String"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution(object):\n",
    "    def reverseWords(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: str\n",
    "        \"\"\"\n",
    "        a = [val for val in s.split(\" \") if val != '']        \n",
    "        a.reverse()\n",
    "        return \" \".join(a)"
   ]
  }
